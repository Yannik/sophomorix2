# begin .htaccess
# www access world wide denied
# DAV and https allowed for student, administrator and teachers
AuthType Basic
AuthName "@@username@@"
SSLRequireSSL
<LimitExcept GET OPTIONS>
  Require user @@username@@ administrator
  Require group @@teachergroup@@
</LimitExcept>
# end .htaccess